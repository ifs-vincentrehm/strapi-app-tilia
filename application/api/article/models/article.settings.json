{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "published_at": {
      "type": "date",
      "default": "2020-06-09T10:00:00.000Z"
    },
    "slug": {
      "type": "string"
    },
    "author": {
      "model": "author",
      "via": "articles"
    },
    "categories": {
      "collection": "category",
      "via": "articles",
      "dominant": true
    }
  }
}
